{
  "name": "svelte-share-buttons-component",
  "sideEffects": false,
  "type": "module",
  "svelte": "src/index.ts",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./src": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "vite",
    "preview": "vite build && vite preview",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.6",
    "rollup": "^4.53.3",
    "rollup-plugin-svelte": "^7.2.3",
    "svelte": "^5.45.2",
    "svelte-check": "^4.3.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ],
  "description": "Simple Svelte based social media share buttons component with no tracking.",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pchynoweth/svelte-share-buttons-component.git"
  },
  "author": "pchynoweth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pchynoweth/svelte-share-buttons-component/issues"
  },
  "homepage": "https://pchynoweth.github.io/svelte-share-buttons-component/",
  "dependencies": {
    "css": "^3.0.0",
    "from": "^0.1.7",
    "import": "^0.0.6"
  }
}
